{   "version": 2, 
    "rewrites": [
            { 
                "source": "/api/pacientes",
                "headers": [
                    {
                        "key": "Authorization",
                        "value": "(.*)"
                    }
                ]
            },
            { 
                "source": "/api/pacientes/:id",
                "headers": [
                    {
                        "key": "Authorization",
                        "value": "(.*)"
                    }
                ]
            },
            { 
                "source": "/api/veterinarios/perfil",
                "headers": [
                    {
                        "key": "Authorization",
                        "value": "(.*)"
                    }
                ]
            },
            { 
                "source": "/api/veterinarios/perfil/:id",
                "headers": [
                    {
                        "key": "Authorization",
                        "value": "(.*)"
                    }
                ]
            },
            { 
                "source": "/api/veterinarios/actualizar-password",
                "headers": [
                    {
                        "key": "Authorization",
                        "value": "(.*)"
                    }
                ]
            },
            { 
                "source": "/api/veterinarios/login",
                "headers": [
                    {
                        "key": "Authorization",
                        "value": "(.*)"
                    }
                ],
                "destination": "/api/pacientes"
            }
    ],
    "redirects": [
        {
            "source": "/api/pacientes",
            "statusCode": 403,
            "destination": "/api/veterinarios/login"
        },
        {
            "source": "/api/pacientes/:id",
            "statusCode": 403,
            "destination": "/api/veterinarios/login"
        },
        {
            "source": "/api/veterinarios/perfil",
            "statusCode": 403,
            "destination": "/api/veterinarios/login"
        },
        {
            "source": "/api/veterinarios/perfil/:id",
            "statusCode": 403,
            "destination": "/api/veterinarios/login"
        },
        {
            "source": "/api/veterinarios/actualizar-password",
            "statusCode": 403,
            "destination": "/api/veterinarios/login"
        },
        {
            "source": "/api/veterinarios/login",
            "statusCode": 403,
            "destination": "/api/veterinarios"
        },
        {
            "source": "/api/veterinarios/login",
            "statusCode": 404,
            "destination": "/api/veterinarios"
        }
    ]
}
